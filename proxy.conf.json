{
  "/api/*": {
    "target": "https://registry.npmjs.org",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/searchapi/*": {
    "target": "https://www.npmjs.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/searchapi": "search"
    }
  }
}
